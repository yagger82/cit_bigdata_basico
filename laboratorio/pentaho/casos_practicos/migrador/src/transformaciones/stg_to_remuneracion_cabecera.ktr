    <connection>PostgreSQL-BIGDATA-LAB</connection>
    <sql>WITH datos_origen AS (
	SELECT
	   tm.periodo_id,
	   fp.funcionario_puesto_id,
	   tm.total_haberes,
	   tm.total_devengado,
	   tm.total_descuentos  
	FROM (
	   SELECT DISTINCT
		  t.periodo_id,
		  f.funcionario_id,
		  s.profesion_id,
		  c.cargo_id,
		  d.estado_id,
		  vrc.anho_ingreso,
		  vrc.total_haberes,
		  vrc.total_devengado,
		  vrc.total_haberes - vrc.total_devengado AS total_descuentos
	   FROM stage.vm_remuneracion_cabecera vrc
	   JOIN sfp.persona p ON p.documento = vrc.documento
	   JOIN sfp.nivel n ON n.codigo = vrc.nivel_codigo
	   JOIN sfp.entidad e ON e.nivel_id = n.nivel_id AND e.codigo = vrc.entidad_codigo
	   JOIN sfp.oee o ON o.entidad_id = e.entidad_id AND o.codigo = vrc.oee_codigo
	   JOIN sfp.funcionario f ON f.persona_id = p.persona_id AND f.oee_id = o.oee_id
	   JOIN sfp.profesion s ON s.codigo = vrc.profesion_codigo
	   JOIN sfp.cargo c ON c.codigo = vrc.cargo_codigo
	   JOIN sfp.estado d ON d.codigo = vrc.estado_codigo
	   JOIN sfp.periodo t ON t.anho = vrc.anho AND t.mes = vrc.mes
	) tm
	JOIN sfp.funcionario_puesto fp ON fp.funcionario_id = tm.funcionario_id AND fp.profesion_id = tm.profesion_id AND fp.cargo_id = tm.cargo_id AND fp.estado_id = tm.estado_id AND fp.anho_ingreso = tm.anho_ingreso
	ORDER BY fp.funcionario_puesto_id
),
datos_target AS (
	SELECT
		rc.remuneracion_cabecera_id,
		rc.periodo_id,
		rc.funcionario_puesto_id
	FROM sfp.remuneracion_cabecera rc
	JOIN sfp.periodo p ON p.periodo_id = rc.periodo_id
	WHERE p.anho = ?::int2
		AND p.mes = ?::int2
	ORDER BY rc.remuneracion_cabecera_id, rc.funcionario_puesto_id
)
SELECT ds.*, dt.remuneracion_cabecera_id
FROM datos_origen ds
LEFT JOIN datos_target dt ON dt.periodo_id = ds.periodo_id AND dt.funcionario_puesto_id = ds.funcionario_puesto_id;</sql>
