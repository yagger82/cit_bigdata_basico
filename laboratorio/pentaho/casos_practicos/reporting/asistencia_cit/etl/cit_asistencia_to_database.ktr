    <sql>

-- CREATE EXTENSION tablefunc;

-- DROP EXTENSION IF EXISTS tablefunc CASCADE;

DO $$
DECLARE
    sql text;
BEGIN

    SET search_path TO reporting;

	--Eliminar tabla temporal viejo
	DROP TABLE IF EXISTS stage.tmp_cit_asistencia;

	--Crear consulta dinamica y crear tabla temporal
    SELECT 'CREATE TABLE stage.tmp_cit_asistencia AS SELECT * FROM stage.crosstab (
        ''select distinct participante, fecha_hora::date as fecha, duracion from raw.raw_cit_asistencia order by 1, 2'',
        ''select distinct fecha_hora::date as fecha from raw.raw_cit_asistencia order by 1''
    ) AS ct(participante varchar(64), ' || string_agg('"' || fecha || '" interval', ', ') || ');'
    INTO sql
    FROM (select distinct fecha_hora::date as fecha from raw.raw_cit_asistencia order by 1) AS sub;

    --RAISE NOTICE 'sql %', sql;

    EXECUTE sql;

	--Eliminar tabla de reporte viejo
	DROP TABLE IF EXISTS rpt_cit_asistencia;
	
	--Crear tabla de reporte nuevo
	EXECUTE 'CREATE TABLE rpt_cit_asistencia AS
	 SELECT 
		ma.apellido, 
		ma.nombre,
		ra.* 
	FROM raw.raw_cit_matriculado ma
	LEFT JOIN stage.tmp_cit_asistencia ra ON ra.participante = ma.participante
	ORDER BY ma.apellido, ma.nombre;';

END $$;</sql>
